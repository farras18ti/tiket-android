        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.AndoirdTiket">
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.TableCinepolis"/>
        <activity android:name=".ui.RegisterActivity"/>
        <activity android:name=".ui.ListFilmActivity"/>
        <activity android:name=".ui.DaftarBioskop"/>
        <activity android:name=".ui.PenjualanActivity"/>
        <activity android:name=".ui.TableBoxMovie"/>
        <activity android:name=".ui.TableCgv"/>
